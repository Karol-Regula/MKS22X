public class KnightBoard{
    int[][] board;
    int size;
    
    public static void main(String[]args){
	
    }

    public KnightBoard(int side){
	size = side;
	fillBoard(board);
    }

    public void fillBoard(){
	for(int i = 0; i < size; i++){
	    for(int j = 0; j < size; j++){
		board[i][j] == 0;
	    }
	}
    }

}
